{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "lib": [
      "ESNext",
      "DOM"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "typeRoots": [
      "app/@types",
      "node_modules/@types"
    ],
  },
  "include": [
    "app/**/*"
  ]
}