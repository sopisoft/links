{
  "name": "links",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "bun run build:client && bun run build:server",
    "build:client": "vite build --mode client",
    "build:server": "vite build",
    "preview": "wrangler pages dev ./dist",
    "format": "bunx @biomejs/biome format --write ./app",
    "lint": "bunx @biomejs/biome check --apply ./app",
    "deploy": "$npm_execpath run build && wrangler pages deploy ./dist"
  },
  "dependencies": {
    "@misskey-dev/summaly": "^5.1.0",
    "hono": "^4.1.3",
    "honox": "^0.1.9"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.6.1",
    "@cloudflare/workers-types": "^4.20240320.1",
    "@hono/vite-cloudflare-pages": "^0.2.5",
    "typescript": "^5.4.3",
    "vite": "^5.2.2",
    "wrangler": "^3.36.0"
  }
}